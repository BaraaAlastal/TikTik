<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ps.example.tiktik">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ui.HomeActivity"
            android:theme="@style/HomeActivity"></activity>
        <activity
            android:name=".Ui.RegisterActivity"
            android:theme="@style/SignInStyleActivity" />
        <activity
            android:name=".Ui.ForgetPasswordActivity"
            android:theme="@style/SignInStyleActivity" />
        <activity
            android:name=".Ui.SignInActivity"
            android:theme="@style/SignInStyleActivity" />
        <activity
            android:name=".Ui.AdsActivity"
            android:theme="@style/LanguageStyleActivity" />
        <activity
            android:name=".Ui.LanguageActivity"
            android:theme="@style/LanguageStyleActivity" />
        <activity
            android:name=".Ui.SplashActivity"
            android:theme="@style/SplashStyleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>